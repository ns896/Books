        (function(window, document) {
            // cookie reader by Lea Verou: http://lea.verou.me/2009/12/reading-cookies-the-regular-expression-way/
            var cookieManager = {};
            var cookieDomain = '.keysight.com';
            // China Domain changes
            var ssoAuthPath = 'https:\/\/www.keysight.com\/ssoauth';

            cookieManager.getCookie = function getCookie(name) {
                // Escape regexp special characters
                name = name.replace(/([.*+?^=!:()|[\]\/\\])/g, '\\$1');

                const regex = new RegExp('(?:^|;)\\s?' + name + '=(.*?)(?:;|$)', 'i'),
                    match = document.cookie.match(regex);

                return match && unescape(match[1]);
            };

            cookieManager.setCookie = function setCookie(name, value, sameSite,expireTime) {
                if (sameSite=='None')
                    document.cookie = name + '=' + value + '; path=/' + '; domain=' + cookieDomain + '; expires=' + expireTime +';SameSite=None; secure';
                else
                    document.cookie = name + '=' + value + '; path=/' + '; domain=' + cookieDomain + '; expires=' + expireTime;

            };

            var userCredentialsCookie = cookieManager.getCookie('USER_CREDENTIALS');
            var  ssoId= cookieManager.getCookie('ssoid');
            var loggedInCookie = cookieManager.getCookie('IS_LOGGED_IN');
            var currentTimeStamp = Math.floor(new Date().valueOf() / 1000);
            if (userCredentialsCookie=='VALID' && loggedInCookie=='true' ){
                cookieManager.setCookie('USER_CREDENTIALS', userCredentialsCookie,'None');
            }

            // China Domain changes - START
            if(cookieDomain == '.keysight.com.cn' && userCredentialsCookie!='VALID'){

                fetch(ssoAuthPath + "/sso-status", {credentials: 'include'})
                    .then(response => response.json())
                    .then(data => {
                        console.log('Success:', encodeURIComponent(data.AGRL));
                        ssoId = data.ssoid;
                        // if ssoId is available and ssoId < currentTimeStamp then set other cookie values like AGRL email usercred etc.
                        if (ssoId !== null && ssoId !== undefined && ssoId !== '' && ssoId !== 'undefined') {
                            userCredentialsCookie = data.USER_CREDENTIALS;
                            var USER_EMAIL = data.USER_EMAIL;
                            var AGRL = encodeURIComponent(data.AGRL);
                            var milsToMin = 1000 * 60;
                            var expireTime = new Date((5 * milsToMin) + Date.now()).toUTCString();
                            cookieManager.setCookie('USER_CREDENTIALS', userCredentialsCookie,'None');
                            cookieManager.setCookie('ssoid', ssoId);
                            cookieManager.setCookie('USER_EMAIL', USER_EMAIL);
                            cookieManager.setCookie('AGRL', AGRL, expireTime);
                            window.location = 'https:\/\/www.keysight.com\/us\/en\/passive\u002Dsaml.login.html?env=https%3A%2F%2Fwww.keysight.com%2Fus%2Fen%2Fassets%2F7121\u002D1111%2Febooks%2FEngineering\u002Dfor\u002DAdvanced\u002DRadar\u002Dand\u002DElectromagnetic\u002DSpectrum\u002DOperations.pdf';
                        }
                    });
            }
            var AGRLCookie = cookieManager.getCookie('AGRL');
            if(cookieDomain == '.keysight.com.cn' && AGRLCookie==null && userCredentialsCookie=='VALID'){
                fetch(ssoAuthPath + "/sso-status", {credentials: 'include'})
                    .then(response => response.json())
                    .then(data => {
                        console.log('Success:', encodeURIComponent(data.AGRL));
                        var AGRLCookieval = encodeURIComponent(data.AGRL);
                        if (AGRLCookieval!==null && AGRLCookieval!== undefined && AGRLCookieval !== '' && AGRLCookieval !== 'undefined'){
                            var expireTime = new Date((5 * 1000 * 60) + Date.now()).toUTCString();
                            cookieManager.setCookie('AGRL', encodeURIComponent(data.AGRL),'', expireTime);

                        }})
                    .catch((error) => {
                        console.error('Error:', error);
                    });
            }

            // China Domain changes - END

            currentTimeStamp = Math.floor(new Date().valueOf() / 1000);
            // Check if the ssoid cookie is set to valid and we don't have an AEM login token.
            if (ssoId !== null && ssoId !== undefined && ssoId !== '' && ssoId !== 'undefined' && currentTimeStamp < ssoId && !loggedInCookie && cookieDomain != '.keysight.com.cn') {
                // Redirect to passive SAML page.
